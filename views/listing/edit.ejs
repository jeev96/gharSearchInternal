<%- include ("../partials/header") %>

<link rel="stylesheet" href="/css/libs/dropzone.css">

<main>
    <div class="px-3">
        <div class="theme-container">
            <div class="py-3">
                <div class="mdc-card p-3">
                    <div class="mdc-tab-bar-wrapper submit-property">
                        <div class="mdc-tab-bar mb-3">
                            <div class="mdc-tab-scroller">
                                <div class="mdc-tab-scroller__scroll-area">
                                    <div class="mdc-tab-scroller__scroll-content">
                                        <button class="mdc-tab mdc-tab--active" tabindex="0">
                                            <span class="mdc-tab__content">
                                                <span class="mdc-tab__text-label">Basic</span>
                                            </span>
                                            <span class="mdc-tab-indicator mdc-tab-indicator--active">
                                                <span
                                                    class="mdc-tab-indicator__content mdc-tab-indicator__content--underline"></span>
                                            </span>
                                            <span class="mdc-tab__ripple"></span>
                                        </button>
                                        <button class="mdc-tab mdc-tab" tabindex="0">
                                            <span class="mdc-tab__content">
                                                <span class="mdc-tab__text-label">Address</span>
                                            </span>
                                            <span class="mdc-tab-indicator mdc-tab-indicator">
                                                <span
                                                    class="mdc-tab-indicator__content mdc-tab-indicator__content--underline"></span>
                                            </span>
                                            <span class="mdc-tab__ripple"></span>
                                        </button>
                                        <button class="mdc-tab mdc-tab" tabindex="0">
                                            <span class="mdc-tab__content">
                                                <span class="mdc-tab__text-label">Additional</span>
                                            </span>
                                            <span class="mdc-tab-indicator mdc-tab-indicator">
                                                <span
                                                    class="mdc-tab-indicator__content mdc-tab-indicator__content--underline"></span>
                                            </span>
                                            <span class="mdc-tab__ripple"></span>
                                        </button>
                                        <button class="mdc-tab mdc-tab" tabindex="0">
                                            <span class="mdc-tab__content">
                                                <span class="mdc-tab__text-label">Media</span>
                                            </span>
                                            <span class="mdc-tab-indicator mdc-tab-indicator">
                                                <span
                                                    class="mdc-tab-indicator__content mdc-tab-indicator__content--underline"></span>
                                            </span>
                                            <span class="mdc-tab__ripple"></span>
                                        </button>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <form action="/listing/<%= listing._id %>?_method=PUT" method="POST" id="listing-edit-form">
                            <input id="upload-listing-id" type="hidden" name="listingId" value="<%= listing._id %>">
                            <input type="hidden" name="userId" value="<%= listing.author.id %>">
                            <input type="hidden" name="username" value="<%= listing.author.username %>">
                            <input type="hidden" name="createdAt" value="<%= moment(listing.createdAt).valueOf() %>">
                            <div class="tab-content tab-content--active">
                                <div id="sp-basic-form" class="row">
                                    <div class="col-xs-12 p-3">
                                        <h1 class="fw-500 text-center">Basic</h1>
                                    </div>
                                    <div class="col-xs-12 p-2">
                                        <div class="mdc-text-field mdc-text-field--outlined">
                                            <input name="title" value="<%= listing.listingInfo.title %>"
                                                class="mdc-text-field__input">
                                            <div class="mdc-notched-outline">
                                                <div class="mdc-notched-outline__leading"></div>
                                                <div class="mdc-notched-outline__notch">
                                                    <label class="mdc-floating-label">Title</label>
                                                </div>
                                                <div class="mdc-notched-outline__trailing"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 p-2">
                                        <div class="mdc-text-field mdc-text-field--outlined mdc-text-field--textarea">
                                            <textarea name="description" class="mdc-text-field__input" rows="5"><%= listing.listingInfo.description %>
                                            </textarea>
                                            <div class="mdc-notched-outline mdc-notched-outline--upgraded">
                                                <div class="mdc-notched-outline__leading"></div>
                                                <div class="mdc-notched-outline__notch">
                                                    <label class="mdc-floating-label">Description</label>
                                                </div>
                                                <div class="mdc-notched-outline__trailing"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 p-2">
                                        <div class="mdc-text-field mdc-text-field--outlined">
                                            <input name="price" type="number" value="<%= listing.price %>"
                                                class="mdc-text-field__input" required>
                                            <div class="mdc-notched-outline">
                                                <div class="mdc-notched-outline__leading"></div>
                                                <div class="mdc-notched-outline__notch">
                                                    <label class="mdc-floating-label">Price</label>
                                                </div>
                                                <div class="mdc-notched-outline__trailing"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 p-2">
                                        <div class="mdc-select mdc-select--outlined">
                                            <div class="mdc-select__anchor">
                                                <i class="mdc-select__dropdown-icon"></i>
                                                <div class="mdc-select__selected-text"></div>
                                                <div class="mdc-notched-outline">
                                                    <div class="mdc-notched-outline__leading"></div>
                                                    <div class="mdc-notched-outline__notch">
                                                        <label class="mdc-floating-label">Ownership</label>
                                                    </div>
                                                    <div class="mdc-notched-outline__trailing"></div>
                                                </div>
                                            </div>
                                            <div class="mdc-select__menu mdc-menu mdc-menu-surface">
                                                <ul class="mdc-list" name="ownership">
                                                    <li class="mdc-list-item" data-value="">
                                                    </li>
                                                    <li class="mdc-list-item <%= listing.ownership === "FREEHOLD" ? "mdc-list-item--selected": "" %>"
                                                        data-value="FREEHOLD">FREEHOLD</li>
                                                    <li class="mdc-list-item <%= listing.ownership === "LEASEHOLD" ? "mdc-list-item--selected": "" %>"
                                                        data-value="LEASEHOLD">LEASEHOLD</li>
                                                    <li class="mdc-list-item <%= listing.ownership === "CO-OPERATIVE-SOCIETY" ? "mdc-list-item--selected": "" %>"
                                                        data-value="CO-OPERATIVE-SOCIETY">
                                                        CO-OPERATIVE-SOCIETY</li>
                                                    <li class="mdc-list-item <%= listing.ownership === "GPA" ? "mdc-list-item--selected": "" %>"
                                                        data-value="GPA">GPA</li>
                                                    <li class="mdc-list-item <%= listing.ownership === "FREEHOLDAND-REG" ? "mdc-list-item--selected": "" %>"
                                                        data-value="FREEHOLD-AND-REG">
                                                        FREEHOLD-AND-REG
                                                    </li>
                                                    <li class="mdc-list-item <%= listing.ownership === "TRANSFER" ? "mdc-list-item--selected": "" %>"
                                                        data-value="TRANSFER">TRANSFER</li>
                                                    <li class="mdc-list-item <%= listing.ownership === "LOI" ? "mdc-list-item--selected": "" %>"
                                                        data-value="LOI">LOI</li>
                                                    <li class="mdc-list-item <%= listing.ownership === "POA" ? "mdc-list-item--selected": "" %>"
                                                        data-value="POA">POA</li>
                                                    <li class="mdc-list-item <%= listing.ownership === "REGISTRY-DONE" ? "mdc-list-item--selected": "" %>"
                                                        data-value="REGISTRY-DONE">REGISTRY-DONE
                                                    </li>
                                                    <li class="mdc-list-item <%= listing.ownership === "NDC" ? "mdc-list-item--selected": "" %>"
                                                        data-value="NDC">NDC</li>
                                                    <li class="mdc-list-item <%= listing.ownership === "CD-CASE" ? "mdc-list-item--selected": "" %>"
                                                        data-value="CD-CASE">CD-CASE</li>
                                                    <li class="mdc-list-item <%= listing.ownership === "OUSTEE" ? "mdc-list-item--selected": "" %>"
                                                        data-value="OUSTEE">OUSTEE</li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>

                                    <input type="hidden" name="type" value="<%= listing.propertyType.type %>">

                                    <div class="col-xs-12 col-sm-6 p-2">
                                        <div class="mdc-select mdc-select--outlined">
                                            <div class="mdc-select__anchor">
                                                <i class="mdc-select__dropdown-icon"></i>
                                                <div class="mdc-select__selected-text"></div>
                                                <div class="mdc-notched-outline">
                                                    <div class="mdc-notched-outline__leading"></div>
                                                    <div class="mdc-notched-outline__notch">
                                                        <label class="mdc-floating-label">Property Type</label>
                                                    </div>
                                                    <div class="mdc-notched-outline__trailing"></div>
                                                </div>
                                            </div>
                                            <div class="mdc-select__menu mdc-menu mdc-menu-surface">
                                                <ul class="mdc-list" name="subtype">
                                                    <li class="mdc-list-item" data-value="">
                                                    </li>
                                                    <li class="mdc-list-item <%= listing.propertyType.subtype === "FLAT" ? "mdc-list-item--selected": "" %>"
                                                        data-value="FLAT">FLAT</li>
                                                    <li class="mdc-list-item <%= listing.propertyType.subtype === "INDEPENDENT HOUSE" ? "mdc-list-item--selected": "" %>"
                                                        data-value="INDEPENDENT HOUSE">INDEPENDENT
                                                        HOUSE</li>
                                                    <li class="mdc-list-item <%= listing.propertyType.subtype === "PLOT" ? "mdc-list-item--selected": "" %>"
                                                        data-value="PLOT">PLOT</li>
                                                    <li class="mdc-list-item <%= listing.propertyType.subtype === "FLOOR" ? "mdc-list-item--selected": "" %>"
                                                        data-value="FLOOR">FLOOR</li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 p-2">
                                        <div class="mdc-select mdc-select--outlined">
                                            <div class="mdc-select__anchor">
                                                <i class="mdc-select__dropdown-icon"></i>
                                                <div class="mdc-select__selected-text"></div>
                                                <div class="mdc-notched-outline">
                                                    <div class="mdc-notched-outline__leading"></div>
                                                    <div class="mdc-notched-outline__notch">
                                                        <label class="mdc-floating-label">Property Status</label>
                                                    </div>
                                                    <div class="mdc-notched-outline__trailing"></div>
                                                </div>
                                            </div>
                                            <div class="mdc-select__menu mdc-menu mdc-menu-surface">
                                                <ul class="mdc-list" name="tag">
                                                    <li class="mdc-list-item" data-value="">
                                                    </li>
                                                    <li class="mdc-list-item <%= listing.listingInfo.tags.indexOf("FOR SALE") > -1 ? "mdc-list-item--selected": "" %>"
                                                        data-value="FOR SALE">FOR SALE</li>
                                                    <li class="mdc-list-item <%= listing.listingInfo.tags.indexOf("FOR RENT") > -1 ? "mdc-list-item--selected": "" %>"
                                                        data-value="FOR RENT">FOR RENT</li>
                                                    <li class="mdc-list-item <%= listing.listingInfo.tags.indexOf("SOLD") > -1 ? "mdc-list-item--selected": "" %>"
                                                        data-value="SOLD">SOLD</li>
                                                    <li class="mdc-list-item <%= listing.listingInfo.tags.indexOf("NEW") > -1 ? "mdc-list-item--selected": "" %>"
                                                        data-value="NEW">NEW</li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-6 py-3 row middle-xs">
                                        <div class="mdc-switch">
                                            <div class="mdc-switch__track"></div>
                                            <div class="mdc-switch__thumb-underlay">
                                                <div class="mdc-switch__thumb">
                                                    <input name="isFeatured" type="checkbox" id="featured"
                                                        <%= listing.listingInfo.tags.indexOf("FEATURED") > -1 ? "checked": "" %>
                                                        class="mdc-switch__native-control">
                                                </div>
                                            </div>
                                        </div>
                                        <label for="featured" class="mx-2">Featured</label>
                                    </div>
                                    <div class="col-xs-6 py-3 row middle-xs">
                                        <div class="mdc-switch">
                                            <div class="mdc-switch__track"></div>
                                            <div class="mdc-switch__thumb-underlay">
                                                <div class="mdc-switch__thumb">
                                                    <input name="isHotOffer" type="checkbox" id="featured"
                                                        <%= listing.listingInfo.tags.indexOf("HOT OFFER") > -1 ? "checked": "" %>
                                                        class="mdc-switch__native-control">
                                                </div>
                                            </div>
                                        </div>
                                        <label for="featured" class="mx-2">Hot Offer</label>
                                    </div>

                                    <div class="col-xs-12 p-3">
                                        <h1 class="fw-500 text-center">Builder Info</h1>
                                    </div>
                                    <div class="col-xs-12 col-sm-12 p-2">
                                        <div class="mdc-select mdc-select--outlined">
                                            <div class="mdc-select__anchor">
                                                <i class="mdc-select__dropdown-icon"></i>
                                                <div class="mdc-select__selected-text"></div>
                                                <div class="mdc-notched-outline">
                                                    <div class="mdc-notched-outline__leading"></div>
                                                    <div class="mdc-notched-outline__notch">
                                                        <label class="mdc-floating-label">Builder Name*</label>
                                                    </div>
                                                    <div class="mdc-notched-outline__trailing"></div>
                                                </div>
                                            </div>
                                            <div class="mdc-select__menu mdc-menu mdc-menu-surface">
                                                <ul class="mdc-list" name="builderId">
                                                    <li class="mdc-list-item mdc-list-item--selected" data-value="">
                                                    </li>
                                                    <li class="mdc-list-item <%= listing.dealerId === "id1" ? "mdc-list-item--selected": "" %>"
                                                        data-value="id1">ATS</li>
                                                    <li class="mdc-list-item <%= listing.dealerId === "id1" ? "mdc-list-item--selected": "" %>"
                                                        data-value="id2">DLF</li>
                                                    <li class="mdc-list-item <%= listing.dealerId === "id1" ? "mdc-list-item--selected": "" %>"
                                                        data-value="id3">HERO</li>
                                                    <li class="mdc-list-item <%= listing.dealerId === "id1" ? "mdc-list-item--selected": "" %>"
                                                        data-value="id3">OTHER</li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 p-3">
                                        <h1 class="fw-500 text-center">Dealer Info</h1>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 p-2">
                                        <div class="mdc-text-field mdc-text-field--outlined">
                                            <input name="dealerName" value="<%= listing.contactInfo.name %>"
                                                class="mdc-text-field__input">
                                            <div class="mdc-notched-outline">
                                                <div class="mdc-notched-outline__leading"></div>
                                                <div class="mdc-notched-outline__notch">
                                                    <label class="mdc-floating-label">Dealer Name</label>
                                                </div>
                                                <div class="mdc-notched-outline__trailing"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 p-2">
                                        <div class="mdc-text-field mdc-text-field--outlined">
                                            <input name="firmName" value="<%= listing.contactInfo.firmName %>"
                                                class="mdc-text-field__input">
                                            <div class="mdc-notched-outline">
                                                <div class="mdc-notched-outline__leading"></div>
                                                <div class="mdc-notched-outline__notch">
                                                    <label class="mdc-floating-label">Firm Name</label>
                                                </div>
                                                <div class="mdc-notched-outline__trailing"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-4 col-sm-6 p-2">
                                        <div class="mdc-text-field mdc-text-field--outlined">
                                            <input name="dealerEmail" value="<%= listing.contactInfo.email %>" class="mdc-text-field__input" required>
                                            <div class="mdc-notched-outline">
                                                <div class="mdc-notched-outline__leading"></div>
                                                <div class="mdc-notched-outline__notch">
                                                    <label class="mdc-floating-label">Dealer Email</label>
                                                </div>
                                                <div class="mdc-notched-outline__trailing"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-8 col-sm-6 p-2">
                                        <div class="mdc-text-field mdc-text-field--outlined">
                                            <input name="dealerDescription" value="<%= listing.contactInfo.description %>" class="mdc-text-field__input">
                                            <div class="mdc-notched-outline">
                                                <div class="mdc-notched-outline__leading"></div>
                                                <div class="mdc-notched-outline__notch">
                                                    <label class="mdc-floating-label">Description</label>
                                                </div>
                                                <div class="mdc-notched-outline__trailing"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 col-md-4 p-2">
                                        <div class="mdc-select mdc-select--outlined">
                                            <div class="mdc-select__anchor">
                                                <i class="mdc-select__dropdown-icon"></i>
                                                <div class="mdc-select__selected-text"></div>
                                                <div class="mdc-notched-outline">
                                                    <div class="mdc-notched-outline__leading"></div>
                                                    <div class="mdc-notched-outline__notch">
                                                        <label class="mdc-floating-label">Dealer Type</label>
                                                    </div>
                                                    <div class="mdc-notched-outline__trailing"></div>
                                                </div>
                                            </div>
                                            <div class="mdc-select__menu mdc-menu mdc-menu-surface">
                                                <ul class="mdc-list" name="dealerType">
                                                    <li class="mdc-list-item" data-value="">
                                                    </li>
                                                    <li class="mdc-list-item <%= listing.contactInfo.type === "OWNER" ? "mdc-list-item--selected": "" %>"
                                                        data-value="OWNER">OWNER</li>
                                                    <li class="mdc-list-item <%= listing.contactInfo.type === "DEALER" ? "mdc-list-item--selected": "" %>"
                                                        data-value="DEALER">DEALER</li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 col-md-4 p-2">
                                        <div class="mdc-text-field mdc-text-field--outlined">
                                            <input name="dealerPhone"
                                                value="<%= listing.contactInfo.phone && listing.contactInfo.phone.length > 0 ? listing.contactInfo.phone[0] : "" %>"
                                                class="mdc-text-field__input">
                                            <div class="mdc-notched-outline">
                                                <div class="mdc-notched-outline__leading"></div>
                                                <div class="mdc-notched-outline__notch">
                                                    <label class="mdc-floating-label">Phone 1</label>
                                                </div>
                                                <div class="mdc-notched-outline__trailing"></div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 col-md-4 p-2">
                                        <div class="mdc-text-field mdc-text-field--outlined">
                                            <input name="dealerPhone"
                                                value="<%= listing.contactInfo.phone && listing.contactInfo.phone.length > 1 ? listing.contactInfo.phone[1] : "" %>"
                                                class="mdc-text-field__input">
                                            <div class="mdc-notched-outline">
                                                <div class="mdc-notched-outline__leading"></div>
                                                <div class="mdc-notched-outline__notch">
                                                    <label class="mdc-floating-label">Phone 2</label>
                                                </div>
                                                <div class="mdc-notched-outline__trailing"></div>
                                            </div>
                                        </div>
                                    </div>

                                    <div class="col-xs-12 p-3">
                                        <h1 class="fw-500 text-center">Contact Display</h1>
                                    </div>
                                    <div class="col-xs-12 col-sm-6 p-2">
                                        <div class="mdc-select mdc-select--outlined">
                                            <div class="mdc-select__anchor">
                                                <i class="mdc-select__dropdown-icon"></i>
                                                <div class="mdc-select__selected-text"></div>
                                                <div class="mdc-notched-outline">
                                                    <div class="mdc-notched-outline__leading"></div>
                                                    <div class="mdc-notched-outline__notch">
                                                        <label class="mdc-floating-label">Contact Info Visible*</label>
                                                    </div>
                                                    <div class="mdc-notched-outline__trailing"></div>
                                                </div>
                                            </div>
                                            <div class="mdc-select__menu mdc-menu mdc-menu-surface">
                                                <ul class="mdc-list" name="contactDisplayType">
                                                    <li class="mdc-list-item <%= listing.listingInfo.contactDisplay.type === "NONE" ? "mdc-list-item--selected": "" %>"
                                                        data-value="NONE">NONE
                                                    </li>
                                                    <li class="mdc-list-item <%= listing.listingInfo.contactDisplay.type === "BUILDER" ? "mdc-list-item--selected": "" %>"
                                                        data-value="BUILDER">BUILDER</li>
                                                    <li class="mdc-list-item <%= listing.listingInfo.contactDisplay.type === "DEALER" ? "mdc-list-item--selected": "" %>"
                                                        data-value="DEALER">DEALER</li>
                                                </ul>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="col-xs-3 py-3 row middle-xs">
                                        <div class="mdc-switch">
                                            <div class="mdc-switch__track"></div>
                                            <div class="mdc-switch__thumb-underlay">
                                                <div class="mdc-switch__thumb">
                                                    <input name="contactDisplayEmail" type="checkbox"
                                                        <%= listing.listingInfo.contactDisplay.email === true ? "checked" : "" %>
                                                        class="mdc-switch__native-control">
                                                </div>
                                            </div>
                                        </div>
                                        <label for="featured" class="mx-2">Display Email</label>
                                    </div>
                                    <div class="col-xs-3 py-3 row middle-xs">
                                        <div class="mdc-switch">
                                            <div class="mdc-switch__track"></div>
                                            <div class="mdc-switch__thumb-underlay">
                                                <div class="mdc-switch__thumb">
                                                    <input name="contactDisplayPhone" type="checkbox"
                                                        <%= listing.listingInfo.contactDisplay.phone === true ? "checked" : "" %>
                                                        class="mdc-switch__native-control">
                                                </div>
                                            </div>
                                        </div>
                                        <label for="featured" class="mx-2">Display Phone</label>
                                    </div>

                                    <div class="col-xs-12 p-2 mt-3 end-xs">
                                        <button class="mdc-button mdc-button--raised next-tab" type="button">
                                            <span class="mdc-button__ripple"></span>
                                            <span class="mdc-button__label">Next</span>
                                            <i class="material-icons mdc-button__icon">navigate_next</i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                            <div class="tab-content">
                                <div id="sp-address-form" class="row">
                                    <div class="col-xs-12 p-3">
                                        <h1 class="fw-500 text-center">Address</h1>
                                    </div>
                                    <div class="col-xs-12 p-2">
                                        <div
                                            class="mdc-text-field mdc-text-field--outlined mdc-text-field--with-leading-icon">
                                            <i class="material-icons mdc-text-field__icon text-muted">location_on</i>
                                            <input id="maps-search" class="mdc-text-field__input">
                                            <div class="mdc-notched-outline">
                                                <div class="mdc-notched-outline__leading"></div>
                                                <div class="mdc-notched-outline__notch">
                                                    <label class="mdc-floating-label">Location</label>
                                                </div>
                                                <div class="mdc-notched-outline__trailing"></div>
                                            </div>
